{
  "name": "IdenGit/l5-blog",
  "description": "laravel5 blog",
  "minimum-stability": "stable",
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "require": {
    "laravel/framework": "v5.0.1",
    "mobileka/scope-applicator": "1.0.*",
    "barryvdh/laravel-ide-helper": "v2.0.3"
  },
  "repositories": [
    {
      "url": "https://github.com/mobileka/TestDummy",
      "type": "git"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "laracasts/testdummy": "2.3.2",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Denis Khainazarov",
      "email": "wvsden@gmail.com"
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
